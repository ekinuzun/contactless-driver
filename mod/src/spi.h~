

#include "common.h"






typedef struct
{
    volatile UINT32 sysConfig;
    volatile UINT32 sysStatus;
    volatile UINT32 irqStatus;
    volatile UINT32 irqEnable;
    volatile UINT32 wakeupEnable;
    volatile UINT32 syst;
    volatile UINT32 moduleCtrl;
    volatile UINT32 chxConf;
    volatile UINT32 chxStat;
    volatile UINT32 chxCTRL;
    volatile UINT32 tx;
    volatile UINT32 rx;
}SpiReg;







INT32 SpiInit(void);
void SpiSend(UINT8 *senBuf, UINT8 senLen);
UINT8 SpiRead(void);
